
                          "{JOB_ID}": {
                                  "Type": "Task",
                                  "Resource": "{CLOUD_FUNCTION_ID}",
                                  {TIMEOUT_SECONDS_BLOCK}
                                  "Parameters": {
                                       "Penv": "{ENVIRONMENT}",
                                       "PreadInputFrom": "{READ_INPUT_FROM}",
                                       "PcloudFunctionInput.$": "$",
                                       "PcloudFunctionToInvoke": "{CLOUD_FUNCTION_TO_INVOKE}",
                                       "PjobIdentifier": "{JOB_IDENTIFIER}",
                                       "PjobName": "{JOB_NAME}",
                                       "PworkflowName.$": "$$.Execution.Name",
                                       "PlastStep": "{LAST_STEP}"
                                       {CLOUD_FUNCTION_UNLOAD_BLOCK}
                                       {CLOSE_PIPELINE_BLOCK}
                                       {CONTINUE_IF_FAIL_BLOCK}
                                  },
                                  "Retry": [
                                              {
                                                  "ErrorEquals": ["States.ALL"],
                                                  "IntervalSeconds": 30,
                                                  "MaxAttempts": 1,
                                                  "BackoffRate": 1.5
                                              }
                                          ],
                                   "Catch": [
                                              {
                                                "ErrorEquals": ["CustomError"],
                                                "Next": "FailBranch_{LEVEL_ID}_{THREAD_ID}"
                                              },
                                              {
                                                 "ErrorEquals": ["States.ALL"],
                                                 "Next": "FailBranch_{LEVEL_ID}_{THREAD_ID}"
                                              }
                                          ],
                                  {RESULT_PATH_BLOCK}
                                  "Next": "{NEXT_JOB_ID}"
                          }